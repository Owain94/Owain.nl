md-card.margin // @stylint ignore
  margin-top 20px
  margin-bottom 20px
  margin-left 40px
  margin-right 40px

@media screen and (max-width:35.438em)
  md-card.margin // @stylint ignore
    margin-top 20px
    margin-bottom 20px
    margin-left 0
    margin-right 0

.menu
  margin-top 5px

md-icon.dissatisfied // @stylint ignore
  font-size 60px
  margin-bottom 15px

.loading
  display flex
  align-items center
  justify-content center

md-list-item > md-icon // @stylint ignore
  position absolute
  margin-left -350px
  margin-top 36px

.height
  margin-bottom 5px

.github_link
  color white
  font-size 18px
  cursor pointer

@media screen and (min-width: 1470px)
  .code_icon
    color white
    font-size large

@media screen and (max-width: 1470px)
  .code_icon
    display none