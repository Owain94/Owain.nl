{
  "name": "angular-logging",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "webpack-dev-server",
    "build": "npm run build:client",
    "build:all": "npm run clean:all && npm run build:client & npm run build:server",
    "build:all:aot": "npm run clean:all && npm run build:client:aot & npm run build:server:aot",
    "build:client": "./node_modules/webpack/bin/webpack.js --env.client -d",
    "build:client:aot": "./node_modules/webpack/bin/webpack.js --env.aot --env.client",
    "build:server": "./node_modules/webpack/bin/webpack.js --env.server -d && npm run clean:build",
    "build:server:aot": "./node_modules/webpack/bin/webpack.js --env.aot --env.server && npm run clean:build",
    "watch:all": "npm run clean:all && npm run watch:client & npm run watch:server",
    "watch:client": "./node_modules/webpack/bin/webpack.js --env.client -d --watch",
    "watch:server": "./node_modules/webpack/bin/webpack.js --env.server -d --watch",
    "clean:all": "rimraf dist && rimraf src/aot",
    "clean:build": "rimraf dist/out-tsc && rimraf src/aot",
    "clean:dist": "rimraf dist",
    "server": "nodemon dist/main.server.bundle.js",
    "test": "karma start ./karma.conf.js",
    "watch:test": "karma start ./karma.conf.js --auto-watch --no-single-run",
    "ci:test": "npm run test && cat ./coverage/lcov.info | ./node_modules/.bin/codacy-coverage --language typescript && codecov -f coverage/*.json",
    "lint": "tslint --type-check --project tsconfig.app.json && tslint --type-check --project tsconfig.server.json",
    "lint-staged": "lint-staged",
    "stylelint-staged": "stylelint",
    "precommit": "lint-staged",
    "prepush": "npm run lint",
    "preprotractor": "webdriver-manager update",
    "protractor": "npm-run-all -p -r protractor:server protractor:live",
    "protractor:ci": "npm-run-all -p -r protractor:server protractor:delay",
    "protractor:live": "protractor ./protractor.conf.js",
    "protractor:delay": "sleep 3 && protractor ./protractor.conf.js",
    "protractor:server": "http-server dist -p 4200 -c-1 --cors",
    "e2e": "protractor ./protractor.conf.js",
    "pree2e": "webdriver-manager update --standalone false --gecko false --quiet"
  },
  "private": true,
  "dependencies": {
    "@agm/core": "1.0.0-beta.0",
