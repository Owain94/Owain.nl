{
  "name": "owain",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "dist": "ng build --prod --aot",
    "fixdist": "./build.sh",
    "deploy": "./node_modules/.bin/grunt sftp:deploy",
    "hmr": "ng serve --hmr -e=hmr",
    "test": "ng test --watch false",
    "lint": "ng lint",
    "lint-staged": "lint-staged",
    "stylelint-staged": "stylelint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "4.0.2",
    "@angular/common": "4.0.2",
    "@angular/compiler": "4.0.2",
    "@angular/core": "4.0.2",
    "@angular/flex-layout": "2.0.0-rc.1",
    "@angular/forms": "4.0.2",
    "@angular/http": "4.0.2",
    "@angular/material": "2.0.0-beta.2",
    "@angular/platform-browser": "4.0.2",
    "@angular/platform-browser-dynamic": "4.0.2",
    "@angular/router": "4.0.2",
    "angular2-google-maps": "0.17.0",
    "autoprefixer": "6.7.7",
    "core-js": "2.4.1",
    "hammerjs": "2.0.8",
    "ng-http-cache": "1.2.0",
    "rxjs": "5.3.0",
    "web-animations-js": "2.2.3",
    "zone.js": "0.8.5"
  },
  "devDependencies": {
    "@angular/cli": "1.0.0",
    "@angular/compiler-cli": "4.0.2",
    "@angular/service-worker": "1.0.0-beta.8",
    "@angularclass/hmr": "1.2.2",
    "@types/hammerjs": "2.0.34",
    "@types/jasmine": "2.5.47",
    "@types/node": "7.0.12",
    "codelyzer": "3.0.0-beta.4",
    "grunt": "1.0.1",
    "grunt-ssh": "0.12.9",
    "jasmine-core": "2.5.2",
    "jasmine-spec-reporter": "3.2.0",
    "karma": "1.6.0",
    "karma-chrome-launcher": "2.0.0",
    "karma-cli": "1.0.1",
    "karma-coverage-istanbul-reporter": "1.1.0",
    "karma-jasmine": "1.1.0",
    "karma-jasmine-html-reporter": "0.2.2",
    "lint-staged": "3.4.0",
    "pre-commit": "1.2.2",
    "protractor": "5.1.1",
    "stylelint": "7.10.1",
    "stylelint-config-standard": "16.0.0",
    "ts-node": "3.0.2",
    "tslint": "5.1.0",
    "typescript": "2.2.2"
  },
  "greenkeeper": {
    "branchPrefix": "web-dep-update-",
    "label": "web-dep-update"
  },
  "lint-staged": {
    "*.ts": "tslint",
    "*.css": "stylelint"
  },
  "pre-commit": "lint-staged",
  "stylelint": {
    "extends": "stylelint-config-standard",
    "rules": {
      "selector-type-no-unknown": [
        true,
        {
          "ignoreTypes": [
            "/^md-/"
          ]
        }
      ]
    }
  }
}
